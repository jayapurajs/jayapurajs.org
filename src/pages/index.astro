---
import Layout from "../layouts/Layout.astro";
---

<Layout title="JayapuraJS Â· Jayapura JavaScript User Group" description="Come and meet other developers interested in JavaScript and its ecosystem in Jayapura. Join our community and stay tuned for future events.">
	<main class="mx-auto max-w-screen-xl">
		<!-- Hero -->
		<div
			class="relative overflow-hidden before:absolute before:top-0 before:left-1/2 before:bg-[url('./polygon.svg')] before:bg-no-repeat before:bg-top before:bg-cover before:w-full before:h-full before:-z-[1] before:transform before:-translate-x-1/2"
		>
			<div class="max-w-[85rem] mx-auto px-4 sm:px-6 lg:px-8 pt-24 pb-10">
				<!-- Icon Hero -->
				<div class="flex justify-center">
					<a href="/" class="sm:mx-auto">
						<div
							class="flex items-center justify-center w-12 h-12 rounded-full bg-teal-accent-400"
						>
							<svg class="w-10 h-10 text-amber-400" stroke="currentColor" viewBox="0 0 52 52">
								<polygon stroke-width="3" stroke-linecap="round" stroke-linejoin="round" fill="none" points="29 13 14 29 25 29 23 39 38 23 27 23"></polygon>
							  </svg>
						</div>
					</a>
				</div>
				<!-- End Icon Hero -->

				<!-- Title -->
				<div class="mt-5 max-w-2xl text-center mx-auto">
					<h1
						class="block font-bold font-rubik text-gray-800 text-4xl md:text-5xl lg:text-6xl"
					>
						Jayapura
						<span
							class="bg-clip-text bg-gradient-to-tl from-amber-400 to-yellow-500 text-transparent"
							>JavaScript</span
						>
						<br />
						 User Group
					</h1>
				</div>
				<!-- End Title -->

				<div class="mt-5 max-w-3xl text-center mx-auto">
					<p class="text-lg text-gray-600 font-rubik">
						Come and meet other developers interested in JavaScript
						and its ecosystem in Jayapura. Join our community and
						stay tuned for future events.
					</p>
				</div>

				<!-- Buttons -->
				<div
					class="mt-8 grid gap-3 w-full sm:inline-flex sm:justify-center"
				>
					<a
						class="inline-flex justify-center items-center gap-x-3.5 py-2 text-sm font-rubik lg:text-base text-center text-white border font-medium hover:border-gray-300 shadow-sm bg-indigo-500 rounded-md focus:outline-none focus:ring-2 focus:ring-gray-400 focus:ring-offset-2 focus:ring-offset-white transition px-4"
						href="https://discord.gg/pYUNzVvN" target="_blank"
					>
						<svg
							xmlns="http://www.w3.org/2000/svg"
							width="24"
							height="24"
							viewBox="0 0 24 24"
							><path
								fill="currentColor"
								d="M19.27 5.33C17.94 4.71 16.5 4.26 15 4a.09.09 0 0 0-.07.03c-.18.33-.39.76-.53 1.09a16.09 16.09 0 0 0-4.8 0c-.14-.34-.35-.76-.54-1.09c-.01-.02-.04-.03-.07-.03c-1.5.26-2.93.71-4.27 1.33c-.01 0-.02.01-.03.02c-2.72 4.07-3.47 8.03-3.1 11.95c0 .02.01.04.03.05c1.8 1.32 3.53 2.12 5.24 2.65c.03.01.06 0 .07-.02c.4-.55.76-1.13 1.07-1.74c.02-.04 0-.08-.04-.09c-.57-.22-1.11-.48-1.64-.78c-.04-.02-.04-.08-.01-.11c.11-.08.22-.17.33-.25c.02-.02.05-.02.07-.01c3.44 1.57 7.15 1.57 10.55 0c.02-.01.05-.01.07.01c.11.09.22.17.33.26c.04.03.04.09-.01.11c-.52.31-1.07.56-1.64.78c-.04.01-.05.06-.04.09c.32.61.68 1.19 1.07 1.74c.03.01.06.02.09.01c1.72-.53 3.45-1.33 5.25-2.65c.02-.01.03-.03.03-.05c.44-4.53-.73-8.46-3.1-11.95c-.01-.01-.02-.02-.04-.02zM8.52 14.91c-1.03 0-1.89-.95-1.89-2.12s.84-2.12 1.89-2.12c1.06 0 1.9.96 1.89 2.12c0 1.17-.84 2.12-1.89 2.12zm6.97 0c-1.03 0-1.89-.95-1.89-2.12s.84-2.12 1.89-2.12c1.06 0 1.9.96 1.89 2.12c0 1.17-.83 2.12-1.89 2.12z"
							></path></svg
						>
						 Discord
					</a>

					<a
						class="inline-flex justify-center items-center gap-x-3.5 text-sm font-rubik text-white lg:text-base text-center border hover:border-gray-300 shadow-sm rounded-md focus:outline-none focus:ring-2 focus:ring-gray-400 focus:ring-offset-2 focus:ring-offset-white transition py-2 px-4 bg-red-600"
						href="#"
					>
						<svg
							xmlns="http://www.w3.org/2000/svg"
							width="24"
							height="32"
							viewBox="0 0 512 512"
							><path
